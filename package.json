{
  "name": "Stevedore",
  "version": "0.0.1",
  "description": "A proxy for docker services",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "lint": "./node_modules/.bin/jshint . --reporter=./node_modules/jshint/src/reporters/default.js",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec -r should",
    "posttest": "./node_modules/.bin/istanbul check-coverage"
  },
  "keywords": [
    "doker",
    "stevedore"
  ],
  "author": "Quirino Brizi - quirino.brizi@gmail.com",
  "license": "BSD-2-Clause",
  "dependencies": {
    "express": "~4.12.4",
    "request": "~2.58.0",
    "underscore": "~1.8.3",
    "cluster": "~0.7.7",
    "querystring": "~0.2.0",
    "body-parser": "~1.13.1",
    "passport": "~0.2.2",
    "passport-http": "~0.2.2",
    "config": "~1.14.0",
    "mocha": "~2.2.5"
  },
  "devDependencies": {
    "mocka": "0.0.1",
    "should": "~7.0.1",
    "sinon": "~1.15.3",
    "nock": "~2.6.0",
    "istanbul": "~0.3.16",
    "jshint": "~2.8.0",
    "jscs": "~1.13.1"
  }
}